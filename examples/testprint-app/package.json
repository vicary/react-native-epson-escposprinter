{
  "name": "testprint-app",
  "version": "0.0.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "android": "expo run:android",
    "arch:new": "cp -f .env.newarch .env.local && expo prebuild --clean",
    "arch:old": "cp -f .env.oldarch .env.local && yarn arch:reset && expo prebuild --clean",
    "arch:reset": "rm -rf ../../node_modules && yarn # New Architecture builds on Android are littering in node_modules",
    "ios": "expo run:ios",
    "lint": "expo lint",
    "reset-project": "node ./scripts/reset-project.js",
    "start": "expo start",
    "test": "jest",
    "web": "expo start --web"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "endingPosition": "absolute-with-indent",
    "jsxSingleQuote": false,
    "plugins": [
      "prettier-plugin-tailwindcss",
      "prettier-plugin-classnames",
      "prettier-plugin-merge"
    ],
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/native": "^7.0.14",
    "expo": "^53.0.11",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.2.0",
    "expo-font": "~13.3.1",
    "expo-linking": "~7.1.5",
    "expo-router": "~5.0.7",
    "expo-splash-screen": "~0.30.9",
    "expo-sqlite": "~15.2.12",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.8",
    "expo-web-browser": "~14.1.6",
    "nativewind": "^4.0.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.3",
    "react-native-epson-escposprinter": "*",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "^0.20.0",
    "tailwindcss": "^3.4.10"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.1",
    "dotenv": "^16.4.5",
    "find-yarn-workspace-root": "^2.0.0",
    "jest": "^29.2.1",
    "jest-expo": "^53.0.7",
    "prettier": "^3.4.2",
    "prettier-plugin-classnames": "^0.7.5",
    "prettier-plugin-merge": "^0.7.2",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "typescript": "^5.8.2",
    "webpack": "^5.59.0"
  }
}
