{
  "name": "testprint-app",
  "version": "0.0.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "android": "expo run:android",
    "arch:new": "cp -f .env.newarch .env.local && expo prebuild --clean",
    "arch:old": "cp -f .env.oldarch .env.local && yarn arch:reset && expo prebuild --clean",
    "arch:reset": "rm -rf ../../node_modules && yarn # New Architecture builds on Android are littering in node_modules",
    "ios": "expo run:ios",
    "lint": "expo lint",
    "reset-project": "node ./scripts/reset-project.js",
    "start": "expo start",
    "test": "jest",
    "web": "expo start --web"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "endingPosition": "absolute-with-indent",
    "jsxSingleQuote": false,
    "plugins": [
      "prettier-plugin-tailwindcss",
      "prettier-plugin-classnames",
      "prettier-plugin-merge"
    ],
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-navigation/native": "^7.0.14",
    "expo": "~52.0.41",
    "expo-constants": "^17.0.4",
    "expo-dev-client": "~5.0.9",
    "expo-font": "^13.0.3",
    "expo-linking": "^7.0.4",
    "expo-router": "^4.0.16",
    "expo-splash-screen": "^0.29.20",
    "expo-sqlite": "~15.1.3",
    "expo-status-bar": "^2.0.1",
    "expo-system-ui": "^4.0.7",
    "expo-web-browser": "^14.0.2",
    "nativewind": "^4.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-native": "^0.76.6",
    "react-native-epson-escposprinter": "*",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.10",
    "tailwindcss": "^3.4.10"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.1",
    "@types/react-test-renderer": "^18.0.7",
    "dotenv": "^16.4.5",
    "find-yarn-workspace-root": "^2.0.0",
    "jest": "^29.2.1",
    "jest-expo": "^52.0.3",
    "prettier": "^3.4.2",
    "prettier-plugin-classnames": "^0.7.5",
    "prettier-plugin-merge": "^0.7.2",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "react-test-renderer": "^18.2.0",
    "typescript-eslint": "^8.20.0"
  }
}
